{
  "name": "saftaartem/apiwrapper",
  "type": "library",
  "description": "A PHP wrapper for using Luminet Project API",
  "keywords": [
    "saftaartem",
    "apiwrapper"
  ],
  "homepage": "https://github.com/ssaftaartem/apiwrapper",
  "license": "MIT",
  "authors": [
    {
      "name": "Artem Safta",
      "email": "safta.artem@gmail.com",
      "homepage": "https://github.com/SaftaArtem/apiWrapper",
      "role": "Developer"
    }
  ],
  "require": {
    "php" : "~7.1"
  },
  "require-dev": {
    "phpunit/phpunit" : ">=8.0",
    "squizlabs/php_codesniffer": "^3.0"
  },
  "autoload": {
    "psr-4": {
      "Lantera\\Safta\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Lantera\\Safta\\Test\\": "tests"
    }
  },
  "scripts": {
    "test": "phpunit",
    "check-style": "phpcs src tests",
    "fix-style": "phpcbf src tests"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0-dev"
    }
  },
  "config": {
    "sort-packages": true
  }
}